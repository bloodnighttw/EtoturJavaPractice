import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		
		Scanner sc=new Scanner(System.in);
		while(sc.hasNext()) {
		int a=sc.nextInt();
		int b=sc.nextInt();
		int c=sc.nextInt();
		if(a+b<=c) {
			System.out.println("unfit");
			break;
		}
		if(a+c<=b) {
			System.out.println("unfit");
			break;
		}
		if(c+b<=a) {
			System.out.println("unfit");
			break;
		}
		System.out.println("fit");
		
		}
		
	}
}
