17
import java.util.*;
public class Main
{
   public static void main(String[] args)
   {
	Scanner sc = new Scanner(System.in);
	int x=0;
	 x =sc.nextInt();
//	Scanner min = new Scanner(System.in);
	 int[] sum=new int[x] ;
	for(int i=0;i<x;i++)	sum[i] = sc.nextInt();
	
	int mins;mins=sum[0];
	
	for(int i = 0;i<x;i++) {
		if(mins>=sum[i])  mins=sum[i];
		
		
	}
	
	
 	System.out.println(mins);	

   
   }
   

   
}
