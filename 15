//15
import java.util.*;
public class Main
{
   public static void main(String[] args)
   {
	Scanner sc = new Scanner(System.in);
	int x=0;
	 x = sc.nextInt();
	 int sum=f1(x);
 	System.out.println(sum);	

   
   }
   
   
   static int f1(int a)   {
	   if(a<=1) return 1;
	   else return a*f1(a-1);
	   
	   
   }
   
}
